Function int DragonRagePower (Void)
{

    //First, check for empty slots
int WepSlot1,WepSlot2,WepSlot3,WepSlot4,DragonTotal,SlotChoice;
Str WN1,WN2,WN3,WN4;
    For(int i = 0; i < MAX_WEAPONS_GLOBAL; i++)
    {
        if(!CheckDragonMove(i)){continue;}
        WN1 = StrParam(s:weapons_ammo[i][0],i:1);
        WN2 = StrParam(s:weapons_ammo[i][0],i:2);
        WN3 = StrParam(s:weapons_ammo[i][0],i:3);
        WN4 = StrParam(s:weapons_ammo[i][0],i:4);

        if(CheckInventory(WN1)){WepSlot1 = 1; DragonTotal++;} 
        if(CheckInventory(WN2)){WepSlot2 = 1; DragonTotal++;}
        if(CheckInventory(WN3)){WepSlot3 = 1; DragonTotal++;}
        if(CheckInventory(WN4)){WepSlot4 = 1; DragonTotal++;}

        if(DragonTotal >=4){break;}
     }
return DragonTotal;

}


Function int CheckDragonMove (int Wep)
{

	switch(Wep)
	{
		Case DragonRage: return 1; break; 
		Case TWISTER: return 1; break;
		Case DRAGONCLAW: return 1; break;
		Case DRAGONPULSE: return 1; break;
		Case DRAGONDANCE: return 1; break;
		Case OUTRAGE: return 1; break;
		Case DRACOMETEOR: return 1; break;
		Case DRAGONBREATH: return 1; break;
		Case DragonRush: return 1; break;
		Case DragonDarts: return 1; break;
	}
	return 0;
}