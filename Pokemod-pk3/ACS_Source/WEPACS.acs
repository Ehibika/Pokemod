Function int GetWepSlot (int Wep)
{ int output = 0;

	for(int i=1;i<=4;i++)
    {
    if(CheckInventory(StrParam(s:weapons_ammo[Wep][0],i:i))){output = i; break;}
    }
return output;
}

Script "GetCurrentWepSlot" (void)
{
	int ThisWeapon = WhichWeapon();
	int ThisSlot = GetWepSlot(ThisWeapon);
	SetResultValue(ThisSlot);
}

